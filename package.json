{
  "name": "vite-plugin-assets-cdn",
  "version": "1.0.0",
  "description": "A vite plugin to import assets as a CDN.",
  "main": "index.js",
  "scripts": {
    "dev": "tsc -w",
    "build": "pnpm lint:fix && rimraf dist && tsc && pnpm build:es",
    "build:es": "tsc --module ESNEXT --outDir dist/es",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "format": "prettier -w src/**/*.* examples/**/*.*",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "vite-plugin",
    "cdn"
  ],
  "author": "zuojiangtao",
  "license": "ISC",
  "dependencies": {
    "rollup-plugin-external-globals": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.{ts,js,json,tsx,jsx,vue}": [
      "prettier --write",
      "eslint --cache --fix",
      "eslint"
    ]
  }
}
